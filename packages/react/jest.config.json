{
  "roots": ["./tests"],
  "preset": "ts-jest",
  "verbose": true,
  "transform": {
    "^.+\\.tsx?$": "ts-jest",
    "^.+\\.(js|jsx)$": "babel-jest"
  },
  "moduleNameMapper": {
    "\\.(css|less|scss|sass)$": "identity-obj-proxy" 
  },
  "moduleFileExtensions": ["ts", "tsx", "js", "jsx"],
  "setupFilesAfterEnv": ["./tests/jest-setup.js"],
  "testPathIgnorePatterns": ["node_modules/", "build/"]
}